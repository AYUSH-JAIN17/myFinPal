{
  "name": "myfinpal",
  "version": "1.0.0",
  "description": "MyFinPal - Your Personal Finance Buddy. Track expenses, budgets, and get spending insights",
  "type": "module",
  "main": "build/mcp/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start:mcp": "node build/mcp/index.js",
    "start:api": "node build/api/index.js",
    "dev:mcp": "tsc && node build/mcp/index.js",
    "dev:api": "tsc && node build/api/index.js",
    "inspect": "npx @modelcontextprotocol/inspector node build/mcp/index.js"
  },
  "keywords": [
    "mcp",
    "finance",
    "budget",
    "expenses",
    "personal-finance"
  ],
